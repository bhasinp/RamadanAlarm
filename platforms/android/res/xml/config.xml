<?xml version='1.0' encoding='utf-8'?>
<widget id="de.appplant.localnotification.example" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Toast">
        <param name="android-package" value="nl.xservices.plugins.Toast" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="LocalNotification">
        <param name="android-package" value="de.appplant.cordova.plugin.localnotification.LocalNotification" />
    </feature>
    <name>Ramadan Alarm</name>
    <description>
       Ramadan Alarm application to remind you at the right time.
    </description>
    <author email="into@initto.ca" href="http://www.initto.ca">
        Initto IT Consulting Inc.
    </author>
    <content src="default.html" />
    <access origin="*" />
</widget>
